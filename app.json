{
   "expo": {
      "name": "react-native-general",
      "slug": "react-native-general",
      "version": "1.0.0",
      "scheme": "com.randyproxz.reactnativegeneral",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "userInterfaceStyle": "light",
      "splash": {
         "image": "./assets/splash.png",
         "resizeMode": "contain",
         "backgroundColor": "#ffffff"
      },
      "updates": {
         "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": [
         "**/*"
      ],
      "ios": {
         "supportsTablet": true,
         "bundleIdentifier": "com.randyproxz.reactnativegeneral",
         "infoPlist": {
            "NSCameraUsageDescription": "This app uses the camera to scan barcodes."
         }
      },
      "android": {
         "adaptiveIcon": {
            "foregroundImage": "./assets/adaptive-icon.png",
            "backgroundColor": "#FFFFFF"
         },
         "package": "com.randyproxz.reactnativegeneral"
      },
      "web": {
         "favicon": "./assets/favicon.png"
      }
   }
}